<h1>Users</h1>
<% @users.each do |user| %>
<table>
  <thead>
    <th>image</th>
    <th>name</th>
  </thead>
  <tbody>
    <tr>
      <td><%= user.image %></td>
      <td><%= user.name %></td>
      <td>
        <span><%= link_to "show", user_path(user.id) %></span>
      </td>
    </tr>
  </tbody>
</table>
<% end %>